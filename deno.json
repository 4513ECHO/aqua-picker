{
  "tasks": {
    "start": "deno run -A --watch=static/,routes/ dev.ts",
    "generate-type": "quicktype -l typescript -s schema https://raw.githubusercontent.com/aquaproj/aqua/main/json-schema/registry.json --just-types --prefer-unions > types/aqua.d.ts"
  },
  "importMap": "./import_map.json",
  "fmt": {
    "files": {
      "exclude": [
        "types/aqua.d.ts"
      ]
    }
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  }
}
